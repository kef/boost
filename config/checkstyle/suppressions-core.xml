<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.0//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">
<suppressions>
    <!-- FIXME: SC502 Fix problems and then remove these suppressions. -->
    <suppress checks="RedundantModifier" files=".*.java"/>
    <suppress checks="LineLength" files=".*.java"/>
    <suppress checks="CyclomaticComplexity" files=".*.java"/>
    <suppress checks="JavaNCSS" files=".*.java"/>
    <suppress checks="ReturnCount" files=".*.java"/>
    <suppress checks="IllegalType" files=".*.java"/>
    <suppress checks="InterfaceIsType" files=".*.java"/>
    <suppress checks="VisibilityModifier" files=".*.java"/>
</suppressions>
